{
    "name": "whotrades.com/deploy",
    "type": "service",
    "description": "Deploy scripts of whotrades.com",
    "repositories": [
        {
            "type": "composer",
            "url": "https://git.whotrades.net/external/packages.json"
        },
        {
            "packagist": false
        }
    ],

    "autoload": {
        "psr-4": {
            "RavenSystem\\": "../../lib/RavenSystem/"
        }
    },

    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "sentry/sentry": "0.16.0"
    }
}
