{
    "name": "whotrades/deploy",
    "type": "service",
    "description": "Deploy scripts of whotrades.com",
    "repositories": [
        {
            "type": "composer",
            "url": "https://artifact.finam.ru/artifactory/api/composer/packagist-org"
        },
        {
            "packagist": false
        }
    ],
    "autoload": {
        "psr-4": {
            "RavenSystem\\": "../../lib/RavenSystem/",
            "RdsSystem\\": "../../lib/RdsSystem/",
            "Cronjob\\": "../../lib/Cronjob/",
            "ServiceBase\\": "../../lib/libcore/"
        },
        "psr-0": {
            "RequestHandler_": "../../lib/libcore/",
            "Monitoring_": "../../lib/libcore/",
            "CoreLight": "../../lib/libcore/",
            "FatalWatcher": "../../lib/libcore/",
            "Utils_": "../../lib/libcore/",
            "": "lib"
        },
        "classmap": ["misc/cron"]
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "php-amqplib/php-amqplib": "~2.6",
        "sentry/sentry": "1.1.0"
    }
}
